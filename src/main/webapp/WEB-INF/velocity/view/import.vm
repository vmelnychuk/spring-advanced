<h1>Import data</h1>
#parse("/WEB-INF/velocity/fragments/_go_back.vm")
<form method="post" enctype="multipart/form-data">
    <table>
        <tr>
            <td>Events file</td>
            <td><input type="file" name="eventFile"/></td>
        </tr>
        <tr>
            <td>Users file</td>
            <td><input type="file" name="userFile"/></td>
        </tr>
        <tr>
            <td>Auditoriums file</td>
            <td><input type="file" name="auditoriumFile"/></td>
        </tr>
        <tr>
            <td>Assigned events file</td>
            <td><input type="file" name="assignedFile"/></td>
        </tr>
        <tr>
            <td>Tickets file</td>
            <td><input type="file" name="ticketFile"/></td>
        </tr>
        <tr>
            <td colspan="2">
                <button type="submit" class="btn btn-primary">Import</button>
            </td>
        </tr>
    </table>
</form>